<!-- Single Article -->
<div class="col-md-12 col-lg-6 col-xl-6 wow animated fadeInUp" data-wow-delay=".3s">
    <article class="single-blog-post">
        <!-- Post Meta -->
        <div class="post-meta">
            <!-- Title -->
            <h2 class="subtitle">%name%</h2>
            <p>%desc%</p>
            <aside>
                <a href="?contactNeeder=%id%">
                    <i class="material-icons icon-round mdi mdi-account-edit"></i>
                </a>
            </aside>
        </div>
    </article>
</div>