<!-- Single Article -->
<div class="col-md-12 col-lg-6 col-xl-6 wow animated fadeInUp" data-wow-delay=".3s">
    <article class="single-blog-post">
        <!-- Post Meta -->
        <div class="post-meta">
            <!-- Title -->
            <h2 class="subtitle">%name%</h2>
            <p>%desc%</p>
            <p>%city%</p>
            <p>%email%</p>
            <p>%phone%</p>
        </div>
    </article>
</div>